{
  "name": "discord-music-bot",
  "version": "1.0.0",
  "description": "TypeScript ile geliştirilmiş Discord müzik botu - Spotify ve YouTube entegrasyonu",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "watch": "tsc -w",
    "deploy": "npm run build && node dist/deploy-commands.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts"
  },
  "keywords": [
    "discord",
    "bot",
    "music",
    "spotify",
    "youtube",
    "typescript"
  ],
  "author": "Discord Music Bot Team",
  "license": "MIT",
  "dependencies": {
    "@discordjs/opus": "^0.9.0",
    "@discordjs/voice": "^0.16.1",
    "axios": "^1.6.2",
    "discord.js": "^14.14.1",
    "dotenv": "^17.2.1",
    "ffmpeg-static": "^5.2.0",
    "spotify-web-api-node": "^5.0.2",
    "tweetnacl": "^1.0.3",
    "winston": "^3.11.0",
    "youtube-dl-exec": "^2.4.13"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.24",
    "@types/spotify-web-api-node": "^5.0.11",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
